<?xml version="1.0" encoding="ISO-8859-1"?>
<project>
  <pomVersion>3</pomVersion>

  <id>dsh-codegen</id>
  <groupId>org.dishevelled</groupId>
  <artifactId>dsh-codegen</artifactId>
  <name>dishevelled.org codegen</name>
  <currentVersion>1.0-SNAPSHOT</currentVersion>

  <organization>
    <name>dishevelled.org</name>
    <url>http://www.dishevelled.org/</url>
  </organization>

  <inceptionYear>2004</inceptionYear>
  <package>org.dishevelled.codegen</package>

  <url>http://www.dishevelled.org</url>
  <siteAddress>www.dishevelled.org</siteAddress>
  <siteDirectory>/home/groups/d/di/dishevelled/htdocs/codegen</siteDirectory>
  <distributionDirectory>/home/groups/d/di/dishevelled/htdocs/codegen/dist</distributionDirectory>

  <repository>
    <connection>scm:cvs:pserver:anonymous@cvs.sourceforge.net:/cvsroot/dishevelled:dishevelled/codegen</connection>
    <developerConnection>scm:cvs:ext:heuermh@cvs.sourceforge.net:/cvsroot/dishevelled:dishevelled/codegen</developerConnection>
    <url>http://cvs.sourceforge.net/viewcvs.py/dishevelled/dishevelled/codegen</url>
  </repository>

  <mailingLists>
    <mailingList>
      <name>dishevelled.org announce mailing list</name>
      <subscribe>dishevelled-announce-subscribe@lists.sourceforge.net</subscribe>
      <unsubscribe>dishevelled-announce-unsubscribe@lists.sourceforge.net</unsubscribe>
    </mailingList>

    <mailingList>
      <name>dishevelled.org cvs commit mailing list</name>
      <subscribe>dishevelled-cvs-subscribe@lists.sourceforge.net</subscribe>
      <unsubscribe>dishevelled-cvs-unsubscribe@lists.sourceforge.net</unsubscribe>
    </mailingList>

    <mailingList>
      <name>dishevelled.org developer mailing list</name>
      <subscribe>dishevelled-devel-subscribe@lists.sourceforge.net</subscribe>
      <unsubscribe>dishevelled-devel-unsubscribe@lists.sourceforge.net</unsubscribe>
    </mailingList>
  </mailingLists>

  <developers>
   <developer>
      <name>Michael Heuer</name>
      <id>heuermh</id>
      <email>heuermh@users.sourceforge.net</email>
      <organization>dishevelled.org</organization>
      <roles>
        <role>Java Developer</role>
      </roles>
    </developer>
  </developers>

  <dependencies>
    <dependency>
      <groupId>commons-collections</groupId><artifactId>commons-collections</artifactId><version>3.0</version>
    </dependency>
    <dependency>
      <groupId>velocity</groupId><artifactId>velocity</artifactId><version>1.4</version>
    </dependency>
    <dependency>
      <groupId>log4j</groupId><artifactId>log4j</artifactId><version>1.2.8</version>
    </dependency>
  </dependencies>

  <build>
    <nagEmailAddress>dishevelled-devel@lists.sourceforge.net</nagEmailAddress>
    <sourceDirectory>src/main/java</sourceDirectory>

    <resources>
      <resource>
        <directory>src/main/resources</directory>
        <includes>
          <include>**/*.wm</include>
        </includes>
      </resource>
    </resources>
  </build>

  <reports>
    <report>maven-jxr-plugin</report>
    <report>maven-javadoc-plugin</report>
    <report>maven-checkstyle-plugin</report>
  </reports>
</project>
