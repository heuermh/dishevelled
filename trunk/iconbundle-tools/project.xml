<?xml version="1.0" encoding="ISO-8859-1"?>
<project>
  <pomVersion>3</pomVersion>

  <id>dsh-iconbundle-tools</id>
  <groupId>org.dishevelled</groupId>
  <artifactId>dsh-iconbundle-tools</artifactId>
  <name>dishevelled.org iconbundle tools</name>
  <currentVersion>SNAPSHOT</currentVersion>

  <organization>
    <name>dishevelled.org</name>
    <url>http://www.dishevelled.org/</url>
  </organization>

  <inceptionYear>2003</inceptionYear>
  <package>org.dishevelled.iconbundle.tools</package>

  <url>http://www.dishevelled.org</url>
  <siteAddress>www.dishevelled.org</siteAddress>
  <siteDirectory>/home/groups/d/di/dishevelled/htdocs/iconbundle</siteDirectory>
  <distributionDirectory>/home/groups/d/di/dishevelled/htdocs/iconbundle/dist</distributionDirectory>

  <repository>
    <connection>scm:cvs:pserver:anonymous@dishevelled.cvs.sourceforge.net:/cvsroot/dishevelled:dishevelled/iconbundle</connection>
    <developerConnection>scm:cvs:ext:heuermh@dishevelled.cvs.sourceforge.net:/cvsroot/dishevelled:dishevelled/iconbundle</developerConnection>
    <url>http://cvs.sourceforge.net/viewcvs.py/dishevelled/dishevelled/iconbundle</url>
  </repository>

  <mailingLists>
    <mailingList>
      <name>dishevelled.org announce mailing list</name>
      <subscribe>dishevelled-announce-subscribe@lists.sourceforge.net</subscribe>
      <unsubscribe>dishevelled-announce-unsubscribe@lists.sourceforge.net</unsubscribe>
    </mailingList>

    <mailingList>
      <name>dishevelled.org cvs commit mailing list</name>
      <subscribe>dishevelled-cvs-subscribe@lists.sourceforge.net</subscribe>
      <unsubscribe>dishevelled-cvs-unsubscribe@lists.sourceforge.net</unsubscribe>
    </mailingList>

    <mailingList>
      <name>dishevelled.org developer mailing list</name>
      <subscribe>dishevelled-devel-subscribe@lists.sourceforge.net</subscribe>
      <unsubscribe>dishevelled-devel-unsubscribe@lists.sourceforge.net</unsubscribe>
    </mailingList>
  </mailingLists>

  <developers>
   <developer>
      <name>Michael Heuer</name>
      <id>heuermh</id>
      <email>heuermh@users.sourceforge.net</email>
      <organization>dishevelled.org</organization>
      <roles>
        <role>Java Developer</role>
      </roles>
    </developer>
  </developers>

  <dependencies>
    <!-- compile-time dependencies -->
    <dependency><groupId>org.dishevelled</groupId><artifactId>dsh-iconbundle</artifactId><version>SNAPSHOT</version></dependency>
    <dependency><groupId>org.dishevelled</groupId><artifactId>dsh-commandline</artifactId><version>SNAPSHOT</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-transcoder</artifactId><version>1.6</version></dependency>

    <!-- runtime dependencies -->
    <dependency><groupId>batik</groupId><artifactId>batik-awt-util</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-bridge</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-css</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-dom</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-ext</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-extension</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-gvt</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-parser</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-rasterizer</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-script</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-svg-dom</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-svggen</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-svgpp</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-ttf2svg</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-util</artifactId><version>1.6</version></dependency>
    <dependency><groupId>batik</groupId><artifactId>batik-xml</artifactId><version>1.6</version></dependency>
    <dependency><groupId>xerces</groupId><artifactId>xercesImpl</artifactId><version>2.6.2</version></dependency>
  </dependencies>

  <build>
    <nagEmailAddress>dishevelled-devel@lists.sourceforge.net</nagEmailAddress>
    <sourceDirectory>src/main/java</sourceDirectory>
    <unitTestSourceDirectory>src/test/java</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
      <excludes>
	<exclude>**/Abstract*</exclude>
      </excludes>
    </unitTest>
  </build>

  <reports>
    <report>maven-jxr-plugin</report>
    <report>maven-javadoc-plugin</report>
    <report>maven-junit-report-plugin</report>
    <report>maven-checkstyle-plugin</report>
    <report>maven-cobertura-plugin</report>
  </reports>
</project>
